<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.nito.ethereald</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/bin/ethereald</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>Nice</key>
	<integer>1</integer>
	<key>UserName</key>
	<string>mobile</string>
	<key>KeepAlive</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/var/mobile/Library/Logs/ethereald-error.log</string>
	<key>StandardOutPath</key>
	<string>/var/mobile/Library/Logs/ethereald.log</string>
</dict>
</plist>
